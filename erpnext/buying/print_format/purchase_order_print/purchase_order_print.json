 "html": "<!DOCTYPE html>\n\t<div >\n        <div class=\"letter-head\">\n                {{letter_head}}\n        </div>\n        <hr/>\n    </div>\n\n    <div style=\"display: flex; margin-bottom: 30px; padding:0px 5px;\">\n        <div style=\"flex:1; float: left; font-size:12px;\">\n            <p>M/s. {{ doc.supplier_name }}</p>\n            <p>{{ frappe.db.get_value(\"Supplier\", doc.supplier_name, \"primary_address\") }}</p>\n            {%- if frappe.db.get_value(\"Supplier\", doc.supplier_name, \"mobile_no\")-%}\n            <p>Tel/Fax: {{ frappe.db.get_value(\"Supplier\", doc.supplier_name, \"mobile_no\") }}</p>\n            {%- endif -%}\n            <p>Email ID: {{frappe.db.get_value(\"Supplier\", doc.supplier_name, \"email_id\") }}</p>\n        </div>\n        <div style=\"flex:1; float: right; font-size:12px; text-align:right\">\n            <strong>\n                {%- if doc.items[0].cost_center -%}\n\t                <strong>{{frappe.db.get_value(\"Cost Center\", {\"name\": doc.items[0].cost_center}, 'company')}}</strong>\n\t                <strong>{{frappe.db.get_value(\"Cost Center\", {\"name\": doc.items[0].cost_center}, 'parent_cost_center')}}</strong>\n                {%- endif -%}\n            </strong>\n        </div>\n    </div>\n    <div style=\"display:flex; font-size:12px;\">\n        <div style=\"flex:1; float:left;\">\n            <table class=\"table_data\">\n                <tr>\n                    <td>Pay-to Vendor No.</td>\n                    <td></td>\n                </tr>\n                <tr>\n                    <td>Vendor Invoice No.</td>\n                    <td></td>\n                </tr>\n                <tr>\n                    <td>Document Date</td>\n                     <td>{{doc.schedule_date}}</td>\n                </tr>\n                <tr>\n                    <td>Invoice No.</td>\n                    <td></td>\n                </tr>\n                 <tr>\n                    <td>Order No.</td>\n                    <td>{{doc.name}}</td>\n                </tr>\n                 <tr>\n                    <td>Posting date</td>\n                    <td>{{doc.transaction_date}}</td>\n                </tr>\n                 <tr>\n                    <td>Due Date</td>\n                    <td>{{doc.schedule_date}}</td>\n                </tr>\n                 <tr>\n                    <td>Payment Terms</td>\n                    <td></td>\n                </tr>\n                 <tr>\n                    <td>Shipping method</td>\n                    <td></td>\n                </tr>\n                 <tr>\n                    <td>Price including VAT</td>\n                    <td></td>\n                </tr>\n            </table>\n        </div>\n        <div style=\"flex:1; float:right; text-align: right;\">\n            <table class=\"table_data\" style=\"text-align:right\" align=\"right\">\n                \n                <tr>\n                    <td>Phone No.</td>\n                    <td><strong>{{ frappe.db.get_value('Company',{'name':doc.company}, 'phone_no') }}</strong></td>\n                </tr>\n                <tr>\n                    <td>Home Page</td>\n                    <td><strong>{{ frappe.db.get_value('Company',{'name':doc.company}, 'website') }}</strong></td>\n                </tr>\n                <tr>\n                    <td>Email</td>\n                    <td><strong>{{ frappe.db.get_value('Company',{'name':doc.company}, 'email') }}</strong></td>\n                </tr>\n                <tr>\n                    <td>VAT Reg No.</td>\n                    <td><strong></strong></td>\n                </tr>\n                 <tr>\n                    <td>Giro No.</td>\n                    <td><strong>{{doc.name}}</strong></td>\n                </tr>\n                 <tr>\n                    <td>Bank</td>\n                    <td><strong>{{frappe.db.get_value('Account', {'name':frappe.db.get_value('Branch', {'name':doc.branch}, 'expense_bank_account' )}, 'bank_name' )}} </strong></td>\n                </tr>\n                 <tr>\n                    <td>Account No.</td>\n                    <td><strong>{{frappe.db.get_value('Account', {'name':frappe.db.get_value('Branch', {'name':doc.branch}, 'expense_bank_account' )}, 'account_number' )}} </strong></td>\n                </tr>\n                \n            </table>\n        </div>\n    </div>\n    <div class=\"body_two\" style=\"margin-bottom: 20px;\">\n        <table class =\"table1\" style=\"font-size:12px;\">\n            <tr id=\"col\">\n                <th id=\"col\"><strong>Item No</strong></th>\n                <th id=\"col\"><strong>Description</strong></th>\n                <th id=\"col\"><strong>TVO No.</strong></th>\n                <th id=\"col\"><strong>QTY</strong></th>\n                <th id=\"col\"><strong>UOM</strong></th>\n                <th id=\"col\"><strong>Direct Unit Cost</strong></th>\n                <th id=\"col\"><strong>Discount%</strong></th>\n                <th id=\"col\"><strong>Allowed Invoice Discount</strong></th>\n                <th id=\"col\"><strong>Line Discount Amt</strong></th>\n                <th id=\"col\"><strong>Amount</strong></th>\n            </tr>\n            {% set ns = namespace(index=1) %}\n            {% for row in doc.items%}\n            {% set ns.index = ns.index + row.idx %}\n            <tr id=\"col\">\n                <td id=\"col\">{{row.item_code}}</td>\n                <td id=\"col\">{{row.description}}</td>\n                <td id=\"col\">{{row.tvo_no}}</td>\n                <td id=\"col\" style=\"text-align:right;\">{{row.qty}}</td>\n                <td id=\"col\">{{row.uom}}</td>\n                <td id=\"col\"></td>\n                <td id=\"col\"></td>\n                <td id=\"col\">{{row.discount_amount}}</td>\n                <td id=\"col\"></td>\n                <td id=\"col\">{{ frappe.format_value(row.amount,{\"fieldtype\":\"Currency\"})}} </td>\n            </tr>\n            {% endfor %}\n            <!--{% for row in doc.taxes%}-->\n            <!--<tr id=\"col\">-->\n            <!--    <td id=\"col\">{{ns.index}}</td>-->\n            <!--    <td id=\"col\">{{row.title}}</td>-->\n            <!--    <td id=\"col\"></td>-->\n            <!--    <td id=\"col\"></td>-->\n            <!--    <td id=\"col\"></td>-->\n            <!--    <td id=\"col\"></td>-->\n            <!--    <td id=\"col\">{{ frappe.format_value(row.tax_amount,{\"fieldtype\":\"Currency\"})}} </td>-->\n            <!--</tr>-->\n            <!--{% endfor %}-->\n            <tr>\n                <td colspan=\"3\" style=\"text-align:right;\">\n                    <b>Total quantity:</b>\n                </td>\n                <td colspan=\"1\" style=\"text-align:right;\">\n                    {{doc.total_qty}}\n                </td>\n            </tr>\n        </table>\n    </div>\n    <div>\n        <table align=\"right\" class=\"table_data\">\n            <tr>\n                <td colspan=\"3\">Excise Amount</td>\n                <td colspan=\"3\" style=\"text-align: right\">0.00</td>\n            </tr>\n            <tr>\n                <td colspan=\"3\">Tax Amount</td>\n                <td colspan=\"3\" style=\"text-align: right\">0.00</td>\n            </tr>\n            <tr>\n                <td colspan=\"3\">Service Tax SHECess Amount</td>\n                <td colspan=\"3\" style=\"text-align: right\">0.00</td>\n            </tr>\n            <tr>\n                <td colspan=\"3\">Other Taxes Amount</td>\n                <td colspan=\"3\" style=\"text-align: right\">0.00</td>\n            </tr>\n            <tr>\n                <td colspan=\"3\">Charges Amount</td>\n                <td colspan=\"3\" style=\"text-align: right\">0.00</td>\n            </tr>\n            <tr>\n                <td colspan=\"3\">Total TDS Including eCess Amount</td>\n                <td colspan=\"3\" style=\"text-align: right\">0.00</td>\n            </tr>\n            <tr>\n                <td colspan=\"3\">Excise Amount</td>\n                <td colspan=\"3\" style=\"text-align: right\">0.00</td>\n            </tr><tr>\n                <td colspan=\"3\">Work Tax Amount</td>\n                <td colspan=\"3\" style=\"text-align: right\">0.00</td>\n            </tr>\n            <tr>\n                <td colspan=\"3\">\n                    <b>Total:</b>\n                </td>\n                <td colspan=\"3\" style=\"text-align: right\">\n                    <b>{{frappe.format_value(doc.grand_total,{\"fieldtype\":\"Currency\"})}}</b>\n                </td>\n            </tr>\n        </table>\n    </div>\n    <!--<div id=\"in_word\">-->\n    <!--    <strong>Amount in Words: </strong> Ngultrum {{ (doc.in_words)[3:] }}-->\n    <!--</div>-->\n    \n    <div>\n        <strong>Ship-to Address</strong>\n        <p></p>\n    </div>\n    \n <!--   <div>-->\n\t<!--\t{%- if doc.terms -%}-->\n\t<!--\t\t<br/><p>Purchase Terms &amp; Conditions:</p>-->\n\t<!--\t\t{{ doc.terms }}-->\n\t<!--\t{%- endif -%}-->\n\t<!--</div>-->\n\t<!--<br />-->\n\t<!--<div>-->\n\t<!--\t{%- if doc.po_footer_text -%}-->\n\t<!--\t\t{{ doc.po_footer_text }}-->\n\t<!--\t{%- endif -%}-->\n\n\t<!--</div>-->\n\t<br/>\n\t<div class=\"pagebreak\"> </div>\n\t\n    <div class=\"footer\">\n        {% if not no_letterhead and footer %}\n        <div class=\"letter-head-footer\">\n          {{ footer }}\n        </div>\n        {% endif %}\n        <p class=\"text-center small page-number visible-pdf\">\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\n        </p>\n    </div>",
