 "font_size": 14,
 "html": "<div id=\"header-html\" class=\"hidden-pdf\">\r\n    {%- if letter_head and not no_letterhead -%}\r\n        <div class=\"letter-head\" style=\"width: 100%;\">\r\n            {{ letter_head }}\r\n        </div>\r\n    {%- endif -%}\r\n</div>\r\n    \r\n<div  style=\"text-align: center;\">\r\n    <h3>Asset Movement</h3>\r\n</div>\r\n\r\n<div>\r\n    <table style=\"width:100%; font-size: 12px;\">\r\n        <tr>\r\n            <td><strong>Purpose:</strong></td> <td>{{doc.purpose}}</td>\r\n            <td><strong>Transaction Date:</strong></td> <td>{{frappe.utils.formatdate(doc.transaction_date)}}</td>\r\n        </tr>\r\n        <tr>\r\n            <td><strong>Transfer Type:</strong></td> <td>{{doc.transfer_type}}</td>\r\n            <td><strong>Reference No.:</strong></td> <td>{{doc.name}}</td>\r\n        </tr>\r\n    </table>\r\n</div> \r\n<div>      \r\n    <table id=\"table2\" class=\"table\" style=\"margin-top: 15px; text-align:center;font-size:small\">\r\n        <!--  Table heading -->\r\n        <tr>\r\n            <td style=\"text-align: center;\">Sl.No</td>\r\n            <td style=\"text-align: center;\">Asset Code</td>\r\n            <td style=\"text-align: center;\">Asset Name</td>\r\n            <td style=\"text-align: center;\">Source Cost Center</td>\r\n            <td style=\"text-align: center;\">Source Custodian</td>\r\n            <td style=\"text-align: center;\">Target Cost Center</td>\r\n            <td style=\"text-align: center;\">Target Custodian</td>\r\n        </tr>                \r\n        <!--  Table data -->\r\n        {%- for row in doc.assets -%}\r\n        <tr>\r\n            <td>{{row.idx}}</td>\r\n            <td>{{row.asset}}</td>\r\n            <td>{{row.asset_name}}\r\n            {% if row.description != None %}\r\n                ({{row.description}})\r\n            {% endif %}\r\n            </td>\r\n            <td>{{row.source_cost_center}}</td>\r\n            <td>\r\n            {% if row.source_custodian_type == 'Employee' %}\r\n                {{row.from_employee}}\r\n                {{row.from_employee_name}}\r\n            {% endif %}\r\n            {% if row.source_custodian_type == 'Desuup' %}\r\n                {{row.from_desuup}}\r\n                {{row.from_desuup_name}}\r\n            {% endif %}\r\n            {% if row.source_custodian_type == 'Other' %}\r\n                {{row.others}}\r\n            {% endif %}\r\n            </td>\r\n            <td>{{row.target_cost_center}}</td>\r\n            <td>\r\n            {% if row.target_custodian_type == 'Employee' %}\r\n                {{row.to_employee}}\r\n                {{row.to_employee_name}}\r\n            {% endif %}\r\n            {% if row.target_custodian_type == 'Desuup' %}\r\n                {{row.to_desuup}}\r\n                {{row.to_desuup_name}}\r\n            {% endif %}\r\n            {% if row.target_custodian_type == 'Other' %}\r\n                {{row.to_other}}\r\n            {% endif %}\r\n            </td>\r\n        </tr>\r\n        {%- endfor -%}\r\n        \r\n    </table>\r\n<div>\r\n\r\n<div id=\"footer-html\" class=\"hidden-pdf\">\r\n        {% if not no_letterhead and footer %}\r\n            <div class=\"letter-head-footer\">\r\n            {{ footer }}\r\n            </div>\r\n        {% endif %}\r\n\r\n        <p class=\"text-center small page-number visible-pdf\">\r\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n        </p>\r\n</div>\r\n",
