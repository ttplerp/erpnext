 "css": ".mytable {\r\n\twidth: 100%; \r\n}\r\n\r\n.mytable, .mytable th, .mytable td {\r\n\tborder: 1px solid #999;\r\n        line-height: 1;   \r\n}\r\n\r\n@media all {\r\n   div .printedby {\r\n       position: fixed;\r\n       bottom: 0;\r\n       text-align: right;\r\n       font-size: xx-small;\r\n       width: 100% !important;\r\n   }\r\n}\r\n\r\n.letter-head img {\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n    display: block;\r\n}",
