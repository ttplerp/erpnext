 "html": "<!DOCTYPE html>\r\n<head>\r\n\t<div >\r\n        <div class=\"letter-head\">\r\n                {{letter_head}}\r\n        </div>\r\n    </div>\r\n</head>\r\n\r\n<body>\r\n    <div class=\"body_one\">\r\n        <p style=\"font-weight:bold;font-size:18px;text-align:center\">{{doc.doctype}}</p>\r\n        <p style=\"font-size:14px;text-align:center;text-decoration:underline\">{{doc.branch}}</p>\r\n    </div>\r\n   <div id=\"div_tab\">\r\n        <table id=\"div_tab\">\r\n            <tr>\r\n                <td><strong>Cost Center:</strong></td>\r\n                <td>{{doc.cost_center}}</td>\r\n                <td><strong>Posting Date:</strong></td>\r\n                <td>{{doc.posting_date}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td><strong>Entry Type:</strong></td>\r\n                <td>{{ doc.purpose}}</td>\r\n                <td><strong>Reference:</strong></td>\r\n                <td>{{doc.name}}</td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <div class=\"body_two\">\r\n        <table class =\"table1\">\r\n            <tr id=\"col\">\r\n                <th id=\"col\"><strong>Sl No</strong></th>\r\n                <th id=\"col\"><strong>Party</strong></th>\r\n                <th id=\"col\"><strong>Invoice No</strong></th>\r\n                <th id=\"col\"><strong>TDS Amount</strong></th>\r\n                <th id=\"col\"><strong>Bill Amount</strong></th>\r\n                <th id=\"col\"><strong>Posting Date</strong></th>\r\n            </tr>\r\n            <tr id=\"col\">\r\n                {% for row in doc.items%}\r\n                <td id=\"col\">{{row.idx}}</td>\r\n                <td id=\"col\">{{row.party}}</td>\r\n                <td id=\"col\">{{row.invoice_no}}</td>\r\n                <td id=\"col\">{{row.tds_amount}}</td>\r\n                <td id=\"col\">{{row.bill_amount}}</td>\r\n                <td id=\"col\">{{row.posting_date}}</td>\r\n            </tr>\r\n            {% endfor %}\r\n        </table>\r\n    </div>\r\n    <div id=\"div_tab\">\r\n        <table id=\"div_tab\">\r\n            <tr>\r\n                <td><strong>Remarks:</strong></td>\r\n                <td>{{doc.remarks}}</td>\r\n                <td><strong>Reference Number:</strong></td>\r\n                <td>{{doc.reference_number}}</td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <div id=\"div_tab\">\r\n        <table id=\"div_tab\">\r\n            <tr>\r\n                <td><strong>Mode of Payment:</strong></td>\r\n                <td>{{doc.mode_of_payment}}</td>\r\n                <td><strong>Credit Account:</strong></td>\r\n                <td>{{doc.credit_account}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td><strong>Total Bill Amount:</strong></td>\r\n                <td>{{ doc.total_amount}}</td>\r\n                <td><strong>Total TDS Amount:</strong></td>\r\n                <td>{{doc.total_tds}}</td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <div class=\"row section-break\" style=\"margin-top: 60px;\">\r\n\t\t<div class=\"col-xs-6 column-break\" style=\"text-align: left;\">Prepared By</div>\r\n\t\t<div class=\"col-xs-6 column-break\" style=\"text-align: center;\">Verified By</div>\r\n\t</div>\r\n\t<div class=\"row section-break\" style=\"margin-top: 60px;\">\r\n\t\t<div class=\"col-xs-6 column-break\" style=\"text-align: left;\">Approved By</div>\r\n\t\t<div class=\"col-xs-6 column-break\" style=\"text-align: center;\">Received By</div>\r\n\t</div>\r\n\t<div><hr></div>\r\n    \r\n    <div class=\"footer\">\r\n        {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n          {{ footer }}\r\n        </div>\r\n        {% endif %}\r\n        <p class=\"text-center small page-number visible-pdf\">\r\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n        </p>\r\n    </div>\r\n</body>",
