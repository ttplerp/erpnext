 "css": "table {\n    margin: 0 auto; /* or margin: 0 auto 0 auto */\n    width: 100%;\n  }\n\n#main {\n   margin-left: 20px;\n   margin-right: 20px;\n}\n\n#table2 th, #table2 td {\n    border: 1px solid black;\n}\n\n@media all {\n   div .printedby {\n       position: fixed;\n       bottom: 0;\n       text-align: center;\n       font-size: xx-small;\n       width: 100% !important;\n   }\n}",
 "custom_format": 1,
 "default_print_language": "en",
