 "doctype": "Print Format",
 "font_size": 14,
 "html": "<div class=\"page-break\">\r\n    <div id=\"header-html\" class=\"hidden-pdf\">\r\n        {%- if letter_head and not no_letterhead -%}\r\n            <div class=\"letter-head\">\r\n                {{ letter_head }}\r\n            </div>\r\n        {%- endif -%}\r\n    </div>\r\n\r\n    <div id=\"main\">\r\n        <div  style=\"text-align: center;\">\r\n            <h3>{{doc.naming_series}}</h3>\r\n            <!--<h5>-->\r\n            <!--    <u>{{doc.branch}}</u>-->\r\n            <!--</h5>-->\r\n        </div>\r\n\r\n        <table class = \"table1\" style=\"margin-top: 1px; text-align:justify;font-size:small; width:100% \">\r\n            <tr>\r\n                <td><strong>Title: </strong> {{doc.title}} </td>\r\n                <td style=\"text-align: right;\">\r\n                    <strong>Posting Date: </strong> {{ frappe.utils.formatdate(doc.posting_date) }} \r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <strong>Entry Type: </strong> {{doc.voucher_type}} \r\n                </td>\r\n                <td style=\"text-align: right;\"> \r\n                    <strong>Reference: </strong> {{doc.name}} \r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td style=\"text-align: left;\">\r\n                    <strong> Branch: </strong>\r\n                    {{doc.branch}}\r\n                </td>\r\n            </tr>\r\n        </table>\r\n        \r\n        <table class=\"table table-condensed\" id=\"table2\" style=\"margin-top: 15px;\">\r\n            <tbody>\r\n                <tr>\r\n                    <!--th>Sr</th-->\r\n                    <th>Account</th>\r\n                    <th>Cost Center</th>\r\n                    <th>Party Type</th>\r\n                    <th>Party</th>\r\n                    <th class=\"text-right\">Debit</th>\r\n                    <th class=\"text-right\">Credit</th>\r\n                </tr>\r\n                {%- for row in doc.accounts -%}\r\n                <tr>\r\n                    <!--td style=\"width: 3%;\">{{ row.idx }}</td-->\r\n                    <td style=\"text-align:left;\">{{ row.account }}</td>\r\n                    <td style=\"\">\r\n                            {{ row.cost_center }}\r\n                    </td>\r\n                    <td style=\"\">\r\n                        {%- if row.party_type -%}\r\n                            {{ row.party_type }}\r\n                        {%- endif -%}\r\n                    </td>\r\n                    <td style=\"\">\r\n                        {%- if row.party -%}\r\n                            {{ row.party }}\r\n                        {%- endif -%}\r\n                    </td>\r\n                    <td style=\"text-align: right;\">{{ '{0:,.2f}'.format(row.debit) }}</td>\r\n                    <td style=\"text-align: right;\">{{ '{0:,.2f}'.format(row.credit) }}</td>\r\n                </tr>\r\n                    {%- if row.apply_tds -%}\r\n                    <tr>\r\n                        <!--td style=\"width: 3%;\">{{ row.idx }}</td-->\r\n                        <td style=\"text-align:left;\">{{ row.tax_account }}</td>\r\n                        <td style=\"\">\r\n                                {{ row.cost_center }}\r\n                        </td>\r\n                        <td style=\"\"></td>\r\n                        <td style=\"\"></td>\r\n                        <td style=\"text-align: right;\">{%- if row.add_deduct_tax == 'Add' -%}{{ '{0:,.2f}'.format(row.tax_amount) }}{%- else -%}0.00{%- endif -%}</td>\r\n                        <td style=\"text-align: right;\">{%- if row.add_deduct_tax == 'Deduct' -%}{{ '{0:,.2f}'.format(row.tax_amount) }}{%- else -%}0.00{%- endif -%}</td>\r\n                    </tr>\r\n                    {%- endif -%}\r\n                {%- endfor -%}\r\n            </tbody>\r\n        </table>\r\n\r\n        <div class=\"row section-break\" style=\"margin-top: 20px;\">\r\n            {% if doc.voucher_type != 'Journal Entry'%}\r\n            <div class=\"col-xs-6 column-break\" style=\"float: left;\">\r\n                <div class=\"row\">\r\n                    <div class=\"col-xs-12 text-left\">\r\n                        <b>Net Amount In Word: </b>{{ (frappe.utils.money_in_words(doc.total_credit))}}\r\n                    </div>\r\n                </div>\r\n                <div class=\"row\">\r\n                    <div class=\"col-xs-12 text-left\">\r\n                        <b>Cheque No.: </b>\r\n                        {%- if doc.cheque_no -%}\r\n                            <span> {{doc.cheque_no}} </span>\r\n                        {%- endif -%}\r\n                    </div>\r\n                </div>\r\n                <div class=\"row\">\r\n                    <div class=\"col-xs-12 text-left\">\r\n                        <b>Cheque Date: </b>\r\n                        {%- if doc.cheque_no -%}\r\n                            <span> {{ frappe.utils.formatdate(doc.cheque_date) }} </span>\r\n                        {%- endif -%}\r\n                    </div>\r\n                </div>\r\n                <div class=\"row\" style=\"margin-top: 4px;\">\r\n                    <div class=\"col-xs-12 text-left\">\r\n                        <b>Clearance Date: </b>\r\n                        {%- if doc.clearance_date -%}\r\n                            <span> {{ frappe.utils.formatdate(doc.clearance_date) }} </span>\r\n                        {%- endif -%}\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            {% endif %}\r\n\r\n            <div class=\"col-xs-6 column-break\" style=\"float: right;\">\r\n                <div class=\"row\">\r\n                    <div class=\"col-xs-12 text-right\">\r\n                        <b>Total Debit: </b> {{doc.get_formatted(\"total_debit\", doc)}}\r\n                    </div>\r\n                </div>\r\n                <div class=\"row\">\r\n                    <div class=\"col-xs-12 text-right\">\r\n                        <b>Total Credit: </b> {{doc.get_formatted(\"total_credit\", doc)}}\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"row section-break\">\r\n            <div style=\"margin-top: 15px; \">\r\n                <div class=\"text-left\" style=\"margin-left: 16px;\">\r\n                    <b>Narration: </b>\r\n                    {%- if doc.user_remark -%}\r\n                        <span>{{doc.user_remark}}</span>\r\n                    {%- endif -%}\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"row section-break\" style=\"margin-top: 10px;\">\r\n            <div class=\"col-xs-6 column-break\" style=\"text-align: left;\">Prepared By: {{frappe.db.get_value(\"User\", doc.owner, \"full_name\")}}</div>\r\n            <div class=\"col-xs-6 column-break\" style=\"text-align: center;\">Verified By</div>\r\n        </div>\r\n\r\n        <div class=\"row section-break\" style=\"margin-top: 10px;\">\r\n            <div class=\"col-xs-6 column-break\" style=\"text-align: left;\">Approved By</div>\r\n            {% if doc.voucher_type != 'Journal Entry'%}\r\n                <!--div class=\"col-xs-6 column-break\" style=\"text-align: center;\">Received By</div-->\r\n             {% endif %}\r\n        </div>\r\n    </div>\r\n\r\n    <div id=\"footer-html\" class=\"visible-pdf\" style=\"position: absolute; bottom: 20px;\">\r\n        {% if not no_letterhead and footer %}\r\n            <div class=\"letter-head-footer\">\r\n            {{ footer }}\r\n            </div>\r\n        {% endif %}\r\n        <p class=\"text-center small page-number visible-pdf\">\r\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n        </p>\r\n    </div>\r\n</div>",
