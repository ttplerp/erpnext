 "css": ".body_one{\n    height:100%;\n    width:100%;\n}\n/*.footer{*/\n/*    position: absolute;*/\n/*    bottom: 0;*/\n/*    margin-left:10%;*/\n/*}*/\n#div_tab{\n    width:100%;\n    color:black;\n    margin-top:10px;\n    font-size:13px;\n}\n.body_two{\n    font-size:14px; \n    color:black; \n    margin-top:20px;\n    text-align:left;\n    line-height: 1.0;\n}\n.table1{\n\ttext-align:justify;\n\tfont-size:small;\n\twidth:100%;\n}\nth{\n    background:#E6E6E6;\n    color:black;\n    font-size:14px;\n}\n#col{\n    border:1px solid #CBCBCB;\n\tborder-collapse: collapse;\n}\n@media all {\n   div .printedby {\n       position: fixed;\n       bottom: 0;\n       text-align: center;\n       font-size: xx-small;\n       width: 100% !important;\n   }\n}",
