 "html": "<!DOCTYPE html>\r\n<div>\r\n    <div class=\"letter-head\">\r\n        {{letter_head}}\r\n    </div>\r\n</div>\r\n\r\n<div>\r\n    <div class=\"body_one\">\r\n        <p style=\"font-weight:bold;font-size:18px;text-align:center\">Bank Payment</p>\r\n        <p style=\"font-size:14px;text-align:center;text-decoration:underline\">{{ doc.branch }}</p>\r\n    </div>\r\n    <div id=\"div_tab\">\r\n        <table id=\"div_tab\">\r\n            <tr>\r\n                <td><strong>Paid From Branch:</strong></td>\r\n                <td>{{doc.branch}}</td>\r\n                <td><strong>Posting Date:</strong></td>\r\n                <td>{{doc.posting_date}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td><strong>Transaction Type:</strong></td>\r\n                <td>{{doc.transaction_type}}</td>\r\n                <td><strong>Payment Type:</strong></td>\r\n                <td>{{doc.payment_type}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td><strong>Transaction No.:</strong></td>\r\n                <td>{{doc.name}}</td>\r\n                <td><strong>Fiscal Year:</strong></td>\r\n                <td>{{doc.fiscal_year}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td><strong>Status:</strong></td>\r\n                <td>{{doc.status}}</td>\r\n                <!--<td><strong>Remarks:</strong></td>-->\r\n                <!--<td>{{doc.remarks}}</td>-->\r\n            </tr>\r\n            <tr>\r\n                <td><strong>Paid From Account:</strong></td>\r\n                <td>{{doc.paid_from}}</td>\r\n                <td><strong>Account Type:</strong></td>\r\n                <td>{{doc.bank_account_type}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td><strong>Bank Name:</strong></td>\r\n                <td>{{doc.bank_name}}</td>\r\n                <td><strong>Account No.:</strong></td>\r\n                <td>{{doc.bank_account_no}}</td>\r\n            </tr>\r\n             <tr>\r\n                <td><strong>Bank Branch:</strong></td>\r\n                <td>{{doc.bank_branch}}</td>\r\n            </tr>\r\n        </table>\r\n        <!--<p><strong>Remarks: </strong>{{doc.remarks}}</p>-->\r\n    </div>\r\n</div>\r\n\r\n <div class=\"body_two\">\r\n    <table class =\"table1\">\r\n        <tr id=\"col\">\r\n            <th id=\"col\"><strong>Sl #</strong></th>\r\n            <th id=\"col\"><strong>Transaction Type</strong></th>\r\n            <th id=\"col\"><strong>Transaction ID</strong></th>\r\n            <th id=\"col\"><strong>Bank Jrl. No.</strong></th>\r\n            <th id=\"col\"><strong>Amount</strong></th>\r\n            <th id=\"col\"><strong>Status</strong></th>\r\n        </tr>\r\n        {% for row in doc.items%}\r\n        <tr id=\"col\">\r\n            <td id=\"col\">{{row.idx}}</td>\r\n            <td id=\"col\">{{row.transaction_type}}</td>\r\n            <td id=\"col\">{{row.transaction_id}}</td>\r\n            <td id=\"col\">{{row.bank_journal_no}}</td>\r\n            <td id=\"col\">{{ frappe.format_value(row.amount, {\"fieldtype\":\"Currency\"}, currency=doc.currency) }}</td>\r\n            <td id=\"col\">{{row.status}}</td>\r\n        </tr>\r\n        {% endfor %}\r\n    </table>\r\n</div>\r\n\r\n<div>\r\n    <div style=\"padding-top: 40px; clear: both;\">\r\n        <div style=\"font-size: small; width: 33%; float: left; text-align:center;\">Total Amount: {{ frappe.format_value(doc.total_amount, {\"fieldtype\":\"Currency\"}, currency=doc.currency) }}</div>\r\n    </div>\r\n</div>\r\n\r\n<div class=\"footer\">\r\n        {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n          {{ footer }}\r\n        </div>\r\n        {% endif %}\r\n        <p class=\"text-center small page-number visible-pdf\">\r\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n        </p>\r\n    </div>",
