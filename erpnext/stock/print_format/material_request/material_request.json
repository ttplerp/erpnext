 "html": "<!DOCTYPE html>\r\n<head>\r\n\t<div >\r\n        <div class=\"letter-head\">\r\n                {{letter_head}}\r\n        </div>\r\n    </div>\r\n</head>\r\n<body>\r\n    <div class=\"body_one\">\r\n        <p style=\"font-weight:bold;font-size:18px;text-align:center\">MATERIAL REQUEST</p>\r\n        <p style=\"font-size:14px;text-align:center;text-decoration:underline\">{{ doc.cost_center }}</p>\r\n    </div>\r\n   <div id=\"div_tab\">\r\n        <table id=\"div_tab\">\r\n            {% if doc.material_request_type == \"Material Transfer\" %}\r\n                <tr>\r\n                    <td><strong>MRT Series:</strong></td>\r\n                    <td>{{doc.name}}</td>\r\n                    <td><strong>MRT Date:</strong></td>\r\n                    <td>{{doc.transaction_date}}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td><strong>MRT Title:</strong></td>\r\n                    <td>{{doc.title}}</td>\r\n                    <td><strong>MR Type:</strong></td>\r\n                    <td>{{doc.material_request_type}}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td><strong>Source Warehouse:</strong></td>\r\n                    <td>{{doc.set_from_warehouse}}</td>\r\n                    <td><strong>Receiving Warehouse:</strong></td>\r\n                    <td>{{doc.set_warehouse}}</td>\r\n                </tr>\r\n            {% endif %}\r\n            {% if doc.material_request_type == \"Material Issue\" %}\r\n                <tr>\r\n                    <td><strong>MRI Series:</strong></td>\r\n                    <td>{{doc.name}}</td>\r\n                    <td><strong>MRI Date:</strong></td>\r\n                    <td>{{doc.transaction_date}}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td><strong>MRI Title:</strong></td>\r\n                    <td>{{doc.title}}</td>\r\n                    <td><strong>MR Type:</strong></td>\r\n                    <td>{{doc.material_request_type}}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td><strong>Source Warehouse:</strong></td>\r\n                    <td>{{doc.set_from_warehouse}}</td>\r\n                    <td><strong>Receiving Warehouse:</strong></td>\r\n                    <td>{{doc.set_warehouse}}</td>\r\n                </tr>\r\n            {% endif %}\r\n        </table>\r\n    </div>\r\n    <div class=\"body_two\">\r\n        <table class =\"table1\">\r\n            <tr id=\"col\">\r\n                <th id=\"col\"><strong>Sl No</strong></th>\r\n                <th id=\"col\"><strong>Item Code</strong></th>\r\n                <th id=\"col\"><strong>Item Name</strong></th>\r\n                <th id=\"col\"><strong>Item Group</strong></th>\r\n                <th id=\"col\"><strong>UoM</strong></th>\r\n                <th id=\"col\"><strong>Qty Received</strong></th>\r\n                <th id=\"col\"><strong>Delivery Date</strong></th>\r\n            </tr>\r\n            <tr id=\"col\">\r\n                {% for row in doc.items%}\r\n                <td id=\"col\">{{row.idx}}</td>\r\n                <td id=\"col\">{{row.item_code}}</td>\r\n                <td id=\"col\">{{row.item_name}}</td>\r\n                <td id=\"col\">{{row.item_group}}</td>\r\n                <td id=\"col\">{{row.uom}}</td>\r\n                <td id=\"col\">{{row.received_qty}}</td>\r\n                <td id=\"col\">{{row.schedule_date}}</td>\r\n            </tr>\r\n            {% endfor %}\r\n        </table>\r\n    </div>\r\n    <div style=\"padding-top: 100px; clear: both;\">\r\n        <div>Authorized Signatory</div>\r\n        <div style=\"padding-top: 80px; width: 50%; float: left; text-align:center;\">(Requested By) </div>\r\n        <div style=\"padding-top: 95px; width: 50%; float: left; text-align:center;\">(Approved By) </div>\r\n    </div>\r\n    <div class=\"footer\">\r\n        {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n          {{ footer }}\r\n        </div>\r\n        {% endif %}\r\n        <p class=\"text-center small page-number visible-pdf\">\r\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n        </p>\r\n    </div>\r\n</body>",
