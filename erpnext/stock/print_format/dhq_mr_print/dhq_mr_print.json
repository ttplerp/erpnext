 "doctype": "Print Format",
 "font_size": 14,
 "html": "<div id=\"header-html\" class=\"hidden-pdf\">\r\n    {%- if letter_head and not no_letterhead -%}\r\n        <div class=\"letter-head\">\r\n            {{ letter_head }}\r\n        </div>\r\n    {%- endif -%}\r\n</div>\r\n\r\n<div id=\"footer-html\" class=\"visible-pdf\">\r\n    {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n        {{ footer }}\r\n        </div>\r\n    {% endif %}\r\n\r\n    <p class=\"text-center small page-number visible-pdf\">\r\n        {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n    </p>\r\n</div>\r\n\r\n<div id=\"main\">\r\n    <div  style=\"text-align: center;\">\r\n        <h3>MATERIAL REQUEST</h3>\r\n        <h5><u>\r\n        {{doc.branch}}\r\n        </u></h5>\r\n    </div>\r\n    <div style=\"margin-top: 10px;\">\r\n       <table class = \"table1\" style=\"margin-top: 1px; text-align:justify;font-size:small; width:100% \">\r\n            <tr>\r\n                <td><strong>Date:</strong></td>\r\n                <td>{{ frappe.utils.formatdate(doc.transaction_date) }}</td>\r\n                <td><strong>Series:</strong></td>\r\n                <td>{{ doc.name }}</td>\r\n            </tr>\r\n            <tr>\r\n                <td><strong>Title:</strong></td>\r\n                <td>{{ doc.title }}</td>\r\n                <td><strong>Status:</strong></td>\r\n                <td colspan='3'>{{ doc.workflow_state }}</td>\r\n            </tr>\r\n            {% if doc.site_name %}\r\n            <tr>\r\n                <td><strong>Site name:</strong></td>\r\n                <td colspan='3'>{{ doc.site_name }}</td>\r\n            </tr>\r\n            {% endif %}\r\n        </table>\r\n\r\n        <table class=\"table table-bordered\" style=\"margin-top: 15px;\">\r\n            <tbody>\r\n                <tr>\r\n                    <th>Sr</th>\r\n                    <th> Code</th>\r\n                    <th> Name </th>\r\n                    <th> Group</th>\r\n                    <th>UoM</th>\r\n                    <th>Quantity</th>\r\n                    <th >Delivery Date</th>\r\n                </tr>\r\n                {%- for row in doc.items -%}\r\n                <tr>\r\n                    <td style=\"width: 3%;\">{{ row.idx }}</td>\r\n                    <td style=\"width: 7%;\">{{ row.item_code }}</td>         \r\n                    <td style=\"width: 45%; text-align: left\">{{ row.item_name }}\r\n                        {%- if frappe.db.get_value(\"Item\", {\"name\": row.item_code}, \"item_sub_group\") -%}\r\n                            <br><i><small>({{frappe.db.get_value(\"Item\", {\"name\": row.item_code}, \"item_sub_group\")}})</small></i>\r\n                        {%- endif -%}\r\n                        {%- if row.notes -%}\r\n                          <br/><small> <b>Specification:</b><br>{{ row.notes}}</small> \r\n                        {%- endif -%}\r\n                    </td>\r\n                    <td style=\"width: 15%;\">{{ row.item_group }}</td>   \r\n                    <td style=\"width: 7%;\">{{ row.uom }}</td>\r\n                    <td style=\"width: 7%;\">{{ row.qty }}</td>\r\n                    <td style=\"width: 16%; text-align: left\">{{ frappe.utils.formatdate(row.schedule_date) }}</td>\r\n                </tr>\r\n                {%- endfor -%}\r\n            </tbody>\r\n        </table>\r\n\r\n        <div style=\"padding-top: 100px; clear: both;\">\r\n            <div style=\"padding-top: 80px; width: 40%; float: left; text-align:left;\">\r\n                <strong>Request by: </strong>\r\n                {%-if doc.owner-%}\r\n                    {{frappe.db.get_value(\"User\", {\"name\":doc.owner},\"full_name\")}} \r\n                {%- endif -%}\r\n                <br>\r\n                {%-if frappe.db.get_value(\"Employee\", {\"user_id\":doc.owner},\"employee_name\") != None -%}\r\n                    <strong style=\"padding-top: 5px\">Department: </strong> \r\n                    {{frappe.db.get_value(\"Employee\", {\"user_id\":doc.owner},\"department\")}} \r\n                {%- endif -%}\r\n            </div>\r\n            <div style=\"padding-top: 80px; width: 60%; float: left; text-align:center;\">\r\n                <strong>(Approved by): </strong> <br>\r\n                <strong style=\"padding-top: 5px\">Director General: </strong> \r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>  ",
