 "html": "<!DOCTYPE html>\r\n<div id=\"header-html\" class=\"hidden-pdf\">\r\n    {%- if letter_head and not no_letterhead -%}\r\n        <div class=\"letter-head\">\r\n            {{ letter_head }}\r\n        </div>\r\n    {%- endif -%}\r\n</div>\r\n\r\n<div id=\"main\">\r\n    <div style=\"text-align: center;\">\r\n        <h3>\r\n            {% if doc.stock_entry_type == \"Material Transfer\" %}\r\n                GOODS TRANSFER NOTE\r\n            {% endif %}\r\n            {% if doc.stock_entry_type == \"Material Receipt\" %}\r\n                GOODS RECEIPT NOTE\r\n            {% endif %}\r\n            {% if doc.stock_entry_type == \"Material Issue\" %}\r\n                GOODS ISSUE NOTE\r\n            {% endif %}\r\n            {% if doc.stock_entry_type == \"Write Off\" %}\r\n                GOODS WRITE OFF NOTE\r\n            {% endif %}\r\n        </h3>\r\n        <h5><u>\r\n            {%- if doc.branch -%}\r\n                {{ frappe.db.get_value(\"Branch\",doc.branch,\"cost_center\") }}\r\n            {%- endif -%}\r\n        </u></h5>\r\n    </div>\r\n    <table style=\"margin-top: 1px; text-align:justify;font-size:small; width:100% \">\r\n        {% if doc.stock_entry_type == \"Material Transfer\" %}\r\n            <tr>\r\n                <td id =\"less\"><strong>Source Warehouse::</strong></td>\r\n                <td id =\"more\">{{doc.from_warehouse}}</td>\r\n                <td id =\"less\"><strong>GTN No::</strong></td>\r\n                <td id =\"more\">{{doc.name}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td id =\"less\"><strong>Receiving Warehouse:</strong></td>\r\n                <td id =\"more\">{{doc.to_warehouse}}</td>\r\n                <td id =\"less\"><strong>GTN Date:</strong></td>\r\n                <td id =\"more\">{{doc.posting_date}}</td>\r\n            </tr>\r\n            {% for row in doc.items%}\r\n            <tr>\r\n                <td id =\"less\"><strong>Material Request:</strong></td>\r\n                <td id =\"more\">{{row.material_request}}</td>\r\n                <td id =\"less\"><strong></strong></td>\r\n                <td id =\"more\"></td>\r\n            </tr>\r\n            {% endfor %}\r\n        {% endif%}\r\n        {% if doc.stock_entry_type == \"Material Receipt\" %}\r\n            <tr>\r\n                <td id =\"less\"><strong>GRN No::</strong></td>\r\n                <td id =\"more\">{{doc.name}}</td>\r\n                <td id =\"less\"><strong>GRN Date:</strong></td>\r\n                <td id =\"more\">{{doc.posting_date}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td id =\"less\"><strong>Receiving Warehouse:</strong></td>\r\n                <td id =\"more\">{{doc.to_warehouse}}</td>\r\n                <td id =\"less\"><strong>Supplier</strong></td>\r\n                <td id =\"more\">{{doc.supplier}}</td>\r\n            </tr>\r\n        {% endif%}\r\n        {% if doc.stock_entry_type == \"Material Issue\" %}\r\n            <tr>\r\n                <td id =\"less\"><strong>GIN No::</strong></td>\r\n                <td id =\"more\">{{doc.name}}</td>\r\n                <td id =\"less\"><strong>GIN Date:</strong></td>\r\n                <td id =\"more\">{{doc.posting_date}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td id =\"less\"><strong>Source Warehouse:</strong></td>\r\n                <td id =\"more\">{{doc.from_warehouse}}</td>\r\n                <td id =\"less\"><strong>Title:</strong></td>\r\n                <td id =\"more\">{{doc.title}}</td>\r\n            </tr>\r\n        {% endif%}\r\n        {% if doc.stock_entry_type == \"Write Off\" %}\r\n            <tr>\r\n                <td id =\"less\"><strong>Title:</strong></td>\r\n                <td id =\"more\">{{doc.title}}</td>\r\n                <td id =\"less\"><strong>SE No::</strong></td>\r\n                <td id =\"more\">{{doc.name}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td id =\"less\"><strong>Source Warehouse:</strong></td>\r\n                <td id =\"more\">{{doc.from_warehouse}}</td>\r\n                <td id =\"less\"><strong>SE Date:</strong></td>\r\n                <td id =\"more\">{{doc.posting_date}}</td>\r\n        \r\n            </tr>\r\n        {% endif%}\r\n    </table>\r\n    \r\n    <table class=\"table table-bordered\" style=\"margin-top: 15px;\" id=\"table2\">\r\n        <thead>\r\n            <tr>\r\n                <th><strong>Sl No</strong></th>\r\n                <th><strong>Item Code</strong></th>\r\n                <th><strong>Item Name</strong></th>\r\n                <th><strong>UOM</strong></th>\r\n                <th><strong>QTY</strong></th>\r\n                <th><strong>Rate</strong></th>\r\n                <th><strong>Amount</strong></th>\r\n            </tr>\r\n        </thead>\r\n        <tbody>\r\n            <tr id=\"col\">\r\n                {% for row in doc.items%}\r\n                <td id=\"col\">{{row.idx}}</td>\r\n                <td id=\"col\">{{row.item_code}}</td>\r\n                <td id=\"col\">{{row.item_name}}</td>\r\n                <td id=\"col\">{{row.uom}}</td>\r\n                <td id=\"col\">{{row.qty}}</td>\r\n                <td id=\"col\">{{row.basic_rate}}</td>\r\n                <td id=\"col\">{{row.amount}}</td>\r\n            </tr>\r\n            {% endfor %}\r\n            <tr id=\"col\">\r\n                <td id=\"col\" colspan=\"6\" style=\"text-align:right;\">\r\n                    <b>Total:</b>\r\n                </td>\r\n                <td id=\"col\">\r\n                    {{doc.total_outgoing_value}}\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    \r\n    <div id=\"in_word\">\r\n        <strong>Amount in Words: </strong> Ngultrum {{ (frappe.utils.money_in_words(doc.total_outgoing_value))[3:] }}\r\n    </div>\r\n    <div style=\"padding-top: 150px; clear: both; font-size: small;\">\r\n        <div>Authorized Signatory</div>\r\n         {% if doc.stock_entry_type == \"Material Transfer\" %}\r\n            <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Transferd By) </div>\r\n            <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Carrier) </div>\r\n            <div style=\"padding-top: 80px; width: 33%; float: right; text-align:center;\">(Received By) </div>\r\n            <div style=\"padding-top: 80px; text-align:center; clear: both;\">(Verified By) </div>\r\n        {% endif %}\r\n        {% if doc.stock_entry_type == \"Material Receipt\" %}\r\n            <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Received By) </div>\r\n            <div style=\"padding-top: 80px; width: 33%; float: right; text-align:center;\">(Verified By) </div>\r\n        {% endif %}\r\n        {% if doc.stock_entry_type == \"Material Issue\" %}\r\n            <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Issue By) </div>\r\n            <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Carrier) </div>\r\n            <div style=\"padding-top: 80px; width: 33%; float: right; text-align:center;\">(Received By) </div>\r\n        {% endif %}\r\n        {% if doc.stock_entry_type == \"Write Off\" %}\r\n            <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Entered By) </div>\r\n            <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Verified By) </div>\r\n            <div style=\"padding-top: 80px; width: 33%; float: right; text-align:center;\">(Approved By) </div>\r\n        {% endif %}\r\n        \r\n    </div>\r\n</div>\r\n\r\n<div id=\"footer-html\" class=\"visible-pdf\" >\r\n    {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n        {{ footer }}\r\n        </div>\r\n    {% endif %}\r\n    <p class=\"text-center small page-number visible-pdf\">\r\n        {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n    </p>\r\n</div>",
