 "html": "<!DOCTYPE html>\r\n<head>\r\n\t<div >\r\n        <div class=\"letter-head\">\r\n                {{letter_head}}\r\n        </div>\r\n    </div>\r\n</head>\r\n\r\n<body>\r\n    <div class=\"body_one\">\r\n        <p style=\"font-weight:bold;font-size:18px;text-align:center\">PURCHASE RECEIPT</p>\r\n        <p style=\"font-size:14px;text-align:center;text-decoration:underline\">\r\n            <strong>\r\n\t            {{ doc.name }}\r\n            </strong>\r\n        </p>\r\n    </div>\r\n    <div id=\"div_tab\">\r\n        <table id=\"div_tab\">\r\n            <tr>\r\n                <td><strong>Supplier:</strong></td>\r\n                <td>{{doc.supplier}}</td>\r\n                <td><strong>Posting Date:</strong></td>\r\n                <td>{{doc.posting_date}}</td>\r\n            </tr>\r\n            {% for row in doc.items%}\r\n            <tr>\r\n                <td><strong>Purchase Order:</strong></td>\r\n                <td>{{row.purchase_order}}</td>\r\n                <td><strong>Schedule Date:</strong></td>\r\n                <td>{{row.schedule_date}} </td>\r\n            </tr>\r\n            {% endfor %}\r\n        </table>\r\n    </div>\r\n    <div class=\"body_two\">\r\n        <table class =\"table1\">\r\n            <tr id=\"col\">\r\n                <th id=\"col\"><strong>Sl No</strong></th>\r\n                <th id=\"col\"><strong>Material Code</strong></th>\r\n                <th id=\"col\"><strong>Material Name</strong></th>\r\n                <th id=\"col\"><strong>QTY</strong></th>\r\n                <th id=\"col\"><strong>UOM</strong></th>\r\n                <th id=\"col\"><strong>Rate</strong></th>\r\n                <th id=\"col\"><strong>Amount Billed</strong></th>\r\n            </tr>\r\n            <tr id=\"col\">\r\n                {% for row in doc.items%}\r\n                <td id=\"col\">{{row.idx}}</td>\r\n                <td id=\"col\">{{row.item_code}}</td>\r\n                <td id=\"col\">{{row.item_name}}</td>\r\n                <td id=\"col\">{{row.qty}}</td>\r\n                <td id=\"col\">{{row.uom}}</td>\r\n                <td id=\"col\">{{row.rate}}</td>\r\n                <td id=\"col\">{{row.amount}} </td>\r\n            </tr>\r\n            {% endfor %}\r\n            <tr id=\"col\">\r\n                <td id=\"col\" colspan=\"6\" style=\"text-align:right;\">\r\n                    <b>Total:</b>\r\n                </td>\r\n                <td id=\"col\">\r\n                    {{doc.grand_total}}\r\n                </td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <div id=\"in_word\">\r\n        <strong>Amount in Words: </strong> Ngultrum {{ (doc.in_words)[3:] }}\r\n    </div>\r\n    <div id=\"div_tab\">\r\n        <table id=\"div_tab\">\r\n            <tr>\r\n                <td>Total Quantity:</td>\r\n                <td>{{doc.total_qty}}</td>\r\n                <td>Total(BTN):</td>\r\n                <td>{{doc.total}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td>Grand Total:</td>\r\n                <td>{{doc.grand_total}}</td>\r\n                <td>Grand Total In Words:</td>\r\n                <td>{{doc.in_words}}</td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <div class=\"footer\">\r\n        {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n          {{ footer }}\r\n        </div>\r\n        {% endif %}\r\n        <p class=\"text-center small page-number visible-pdf\">\r\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n        </p>\r\n    </div>\r\n</body>",
