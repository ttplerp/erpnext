 "doctype": "Print Format",
 "font_size": 14,
 "html": "<div id=\"header-html\" class=\"hidden-pdf\">\r\n    {%- if letter_head and not no_letterhead -%}\r\n    <div class=\"letter-head\">\r\n        {{ letter_head }}\r\n    </div>\r\n    {%- endif -%}\r\n</div>\r\n\r\n<div style=\"text-align: center;\">\r\n    <h3>MATERIAL RECEIPT</h3>\r\n    <h5><u>{{ doc.branch }}</u></h5>\r\n</div>\r\n\r\n<div>\r\n    <table class=\"table1\"\r\n        style=\"margin-top: 1px; text-align:justify;font-size:small; width:100%; text-transform: capitalize; \">\r\n\r\n        <tr>\r\n            <td><strong>MR No.:</strong></td>\r\n            <td>{{ doc.name }}</td>\r\n            <td><strong>MR Date:</strong></td>\r\n            <td>{{ frappe.utils.formatdate(doc.posting_date) }}</td>\r\n        </tr>\r\n        <tr>\r\n            <td><strong>Dispatch Date: </strong></td>\r\n            <td> {%- if doc.vehicle_dispatch_date_and_time -%} {{ doc.vehicle_dispatch_date_and_time }} {%- endif -%}</td>\r\n            <td><strong>Transporter Name: </strong></td>\r\n            <td> {%- if doc.transporter_name -%}  {{ doc.transporter_name }} {%- endif -%}</td>\r\n        </tr>\r\n        <tr>\r\n            <td><strong>Vehicle No:</strong></td>\r\n            <td>{%- if doc.vehicle_no -%}  {{ doc.vehicle_no }} {%- endif -%}</td>\r\n            <td><strong>MR Title:</strong></td>\r\n            <td>{%- if doc.title -%}  {{ doc.title }} {%- endif -%}</td>\r\n        </tr>\r\n    </table>\r\n</div>\r\n\r\n<table class=\"table2\" style=\"margin-top: 15px; text-transform: capitalize;font-size: 12px;\">\r\n    <tbody>\r\n        <tr>\r\n            <th>Sr</th>\r\n            <th style=\"text-align: center;\">Material Code</th>\r\n            <th style=\"text-align: center;\">Material Name</th>\r\n            <th style=\"text-align: center;\">Qty Issued</th>\r\n            <th style=\"text-align: center;\">UoM</th>\r\n            <th style=\"text-align: center;\">Rate</th>\r\n            <th class=\"text-right\" style=\"text-align: center;\">Amount</th>\r\n        </tr>\r\n        {%- for row in doc.items -%}\r\n        <tr>\r\n            <td>{{ row.idx }}</td>\r\n            <td>{{ row.item_code }}</td>\r\n            <td>{{ row.item_name }}\r\n                {%- if frappe.db.get_value(\"Item\", {\"name\": row.item_code}, \"item_sub_group\") -%}\r\n                <br /><i>({{frappe.db.get_value(\"Item\", {\"name\": row.item_code}, \"item_sub_group\")}})</i>\r\n                {%- endif -%}\r\n            </td>\r\n            <td style=\"text-align:right;\">{{ row.qty }}</td>\r\n            <td >{{ row.uom }}</td>\r\n            <td style=\"text-align:right;\">{{ frappe.format_value(row.valuation_rate,{'fieldtype':'Currency'}) }}</td>\r\n            <td style=\"text-align:right;\">{{ frappe.format_value(row.amount,{'fieldtype':'Currency'})}}</td>\r\n        </tr>\r\n        {%- endfor -%}\r\n        <tr>\r\n            <td colspan=\"6\" style=\"text-align:right;\"><b>Total Amount:</b></td>\r\n            <td style=\"text-align:right;\">{{ frappe.format_value(doc.total_amount,{'fieldtype':'Currency'}) }}</td>\r\n        </tr>\r\n    </tbody>\r\n</table>\r\n\r\n<div style=\"float: left; width: 100%; font-size: small; text-transform: capitalize; margin-top: 20px;\">\r\n        <div style=\"text-align: left;\"><strong>In Words: </strong>Ngultrum {{\r\n            (frappe.utils.money_in_words(doc.total_amount))[3:] }}\r\n        </div>\r\n        <div style=\"padding-top: 100px; clear: both; text-transform: capitalize;\">\r\n            <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">Issued by </div>\r\n            <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">Carrier </div>\r\n            <div style=\"padding-top: 80px; width: 33%; float: right; text-align:center;\">Received by<br>\r\n                <b>({{frappe.get_fullname(doc.owner)}})</b>\r\n            </div>\r\n        </div>\r\n</div>\r\n\r\n<div id=\"footer-html\" class=\"visible-pdf\" style=\"position: absolute; bottom: 20px;\">\r\n    {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n        {{ footer }}\r\n        </div>\r\n    {% endif %}\r\n    <p class=\"text-center small page-number visible-pdf\">\r\n        {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n    </p>\r\n</div>\r\n",
