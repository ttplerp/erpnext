 "html": "<!DOCTYPE html>\r\n\t<div >\r\n        <div class=\"letter-head\">\r\n            {{letter_head}}\r\n        </div>\r\n        <div style=\"margin-top: -30px;\">\r\n            <div style=\"font-weight: bold; font-size:14px;\">\r\n               {{doc.company}}\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n   <table class=\"table\" style=\"font-size:12px;\">\r\n        <tr>\r\n            <td style=\"width: 50%\">\r\n                <div style=\"margin-bottom: 10px; margin-left:-10px;\">\r\n        \t        <p style=\"font-weight: bold; font-size:14px;\">Challen Number</p>\r\n        \t        <p id=\"col\" style=\"padding-top: 5px; padding-bottom:5px;\">{{doc.name}}</p>\r\n        \t    </div>\r\n        \t    <div style=\"margin-bottom: 10px; margin-left:-10px;\">\r\n        \t        <p style=\"font-weight: bold; font-size:14px;\">Order Number</p>\r\n        \t        <p id=\"col\" style=\"padding-top: 15px; padding-bottom:15px;\"></p>\r\n        \t    </div>\r\n        \t    \r\n        \t    <div style=\"margin-bottom: 10px; margin-left:-10px;\">\r\n        \t        <p style=\"font-weight: bold; font-size:14px;\">Invoice Number</p>\r\n        \t        <p id=\"col\" style=\"padding-top: 15px; padding-bottom:15px;\"></p>\r\n        \t    </div>\r\n            </td>\r\n            <td style=\"width: 50%\">\r\n                <div style=\"margin-bottom: 10px;\">\r\n        \t        <p style=\"font-weight: bold; font-size:14px;\">Challen Date</p>\r\n        \t        <p id=\"col\" style=\"padding-top: 5px; padding-bottom:5px;\">{{doc.posting_date}}</p>\r\n        \t    </div>\r\n        \t    \r\n        \t    <div style=\"margin-bottom: 10px; \">\r\n        \t        <p style=\"font-weight: bold; font-size:14px;\">Order Date</p>\r\n        \t        <p id=\"col\" style=\"padding-top: 6px; padding-bottom:7px;\">{{doc.posting_date}}</p>\r\n        \t    </div>\r\n        \t    \r\n        \t    <div style=\"margin-bottom: 10px;\">\r\n    