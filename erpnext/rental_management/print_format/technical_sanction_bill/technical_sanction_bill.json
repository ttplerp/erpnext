 "html": "<div class=\"page-break\">\r\n\t{%- if letter_head and not no_letterhead -%}\r\n\t<div class=\"letter-head\">{{ letter_head }}</div>\r\n\t{%- endif -%}\r\n\r\n\t<div id=\"main\">\r\n\t\t<h3 align = \"center\">Technical Sanction Bill</h3>\r\n\t\t<h5 align = \"center\"><u>{{doc.branch}}</u></h5>\r\n\t</div>\r\n\t\r\n\t\r\n\t<div style=\"margin-top: 10px;\">\r\n\t\t<table class = \"table1\" style=\"margin-top: 1px; text-align:justify;font-size:small; width:100% \">\r\n\t\t\t<tr>\r\n\t\t\t\t<td align= \"left\" > <strong>Title:</strong></td>\r\n\t\t\t\t<td align= \"left\">{{doc.title}}</td>\r\n\t\t\t\t<td align= \"left\"><strong>Block No:</strong></td>\r\n\t\t\t\t<td align= \"left\">{{ doc.block_no }}</td>\r\n\t\t\t</tr>\r\n\t\t\t<tr>\r\n\t\t\t\t<td align= \"left\"><strong>Site Supervisor: </strong></td>\r\n\t\t\t\t<td align= \"left\"> {{doc.site_inspector}}</td>\r\n\t\t\t\t<td align= \"left\"><strong>Site Supervisor Name:</strong></td>\r\n\t\t\t\t<td align= \"left\">{{ doc.site_inspector_name }}</td>\r\n\t\t\t</tr>\r\n\t\t\t<tr>\r\n\t\t\t\t<td align= \"left\"><strong>Maintenance Type: </strong></td>\r\n\t\t\t\t<td align= \"left\"> {{doc.maintenance_type}}</td>\r\n\t\t\t\t<td align= \"left\"><strong>Posting date:</strong></td>\r\n\t\t\t\t<td align= \"left\">{{ doc.posting_date }}</td>\r\n\t\t\t</tr>\r\n\t\t\t<tr>\r\n\t\t\t\t<td align= \"left\"><strong>Flat No: </strong></td>\r\n\t\t\t\t<td align= \"left\"> {{doc.flat_no}}</td>\r\n\t\t\t\t<td align= \"left\"><strong>Dzongkhag:</strong></td>\r\n\t\t\t\t<td align= \"left\">{{ doc.dzongkhag }}</td>\r\n\t\t\t</tr>\r\n\t\t\t<tr>\r\n\t\t\t\t<td align= \"left\"><strong>Region: </strong></td>\r\n\t\t\t\t<td align= \"left\"> {{doc.region}}</td>\r\n\t\t\t\t<td align= \"left\"><strong>Location::</strong></td>\r\n\t\t\t\t<td align= \"left\">{{ doc.location }}</td>\r\n\t\t\t</tr>\r\n\t\t\t<tr>\r\n\t\t\t\t<td align= \"left\"><strong>Party Type: </strong></td>\r\n\t\t\t\t<td align= \"left\"> {{doc.party_type}}</td>\r\n\t\t\t\t<td align= \"left\"><strong>Party:</strong></td>\r\n\t\t\t\t<td align= \"left\">{{ doc.party }}</td>\r\n\t\t\t</tr>\r\n\t\t\t\t<tr>\r\n\t\t\t\t<td align= \"left\"><strong>Company:: </strong></td>\r\n\t\t\t\t<td align= \"left\"> {{doc.company}}</td>\r\n\t\t\t\t<td align= \"left\"><strong>Expense Account:</strong></td>\r\n\t\t\t\t<td align= \"left\">{{ doc.expense_account }}</td>\r\n\t\t\t</tr>\r\n\t\t\r\n\t\t</table>\r\n\t\t</div>\r\n\t\t<div>\r\n\t\t<table class = \"table2\" style=\"margin-top: 1px; text-align:justify;font-size:small; width:100% \">\r\n\t\t    <tr>\r\n             <th>Sr</th>\r\n             <th>Particulars</th>\r\n             <th>Unit</th>\r\n             <th>Quantity</th>\r\n             <th>Rate</th>\r\n             <th>Amount</th>\r\n   \r\n             </tr>\r\n             \r\n             \r\n             \r\n              {% for item in doc.items %}\r\n             <tr>\r\n                <td style=\"width: 3%;\">{{ loop.index }}</td>\r\n                <td style=\"width: 80%;\">{{ item.item_name }}</td>\r\n                <td style=\"width: 4%;\">{{ item.uom }}</td>\r\n                <td style=\"width: 4%;\">{{ item.qty }}</td>\r\n                <td style=\"width: 4%;\">{{ item.amount }}</td>\r\n                <td style=\"width: 5%;\">{{ item.total }}</td>\r\n        \r\n            </tr>\r\n            \r\n            {% endfor %}\r\n            \r\n           <tr class=\"noBorder\">\r\n\t\t\t  <td colspan=\"5\" style=\"text-align:right;\"><strong>Total TDS Taxable Amount: :</strong></td>\r\n\t\t\t   <td colspan=\"3\" style=\"text-align:right;\">{{doc.tds_taxable_amount }}</td>\r\n\t\t\t  \r\n\t\t\t</tr>\r\n\t\t\t <tr class=\"noBorder\">\r\n\t\t\t  <td colspan=\"5\" style=\"text-align:right;\"><strong>TDS Amount:</strong></td>\r\n\t\t\t   <td colspan=\"3\" style=\"text-align:right;\">{{doc.tds_amount }}</td>\r\n\t\t\t  \r\n\t\t\t</tr>\r\n\t\t\t <tr class=\"noBorder\">\r\n\t\t\t  <td colspan=\"5\" style=\"text-align:right;\"><strong>Total Gross Amount:</strong></td>\r\n\t\t\t   <td colspan=\"3\" style=\"text-align:right;\">{{doc.total_gross_amount }}</td>\r\n\t\t\t  \r\n\t\t\t</tr>\r\n\t\t\t <tr class=\"noBorder\">\r\n\t\t\t  <td colspan=\"5\" style=\"text-align:right;\"><strong>Total Deduction Amount:</strong></td>\r\n\t\t\t   <td colspan=\"3\" style=\"text-align:right;\">{{doc.total_deduction_amount }}</td>\r\n\t\t\t  \r\n\t\t\t</tr>\r\n\t\t\t <tr class=\"lastBorder\">\r\n\t\t\t  <td colspan=\"5\" style=\"text-align:right;\"><strong>Total Estimate Amount:</strong></td>\r\n\t\t\t   <td colspan=\"3\" style=\"text-align:right;\">{{doc.total_amount }}</td>\r\n\t\t\t  \r\n\t\t\t</tr>\r\n\t\t</table>\r\n\t\t</div>\r\n\t\t    \r\n\t\t\r\n\t</div>",
